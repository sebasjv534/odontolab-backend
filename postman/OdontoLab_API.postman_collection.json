				{
					"name": "Register First Admin",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Si el registro es exitoso, guardar las credenciales",
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    if (response.credentials) {",
									"        pm.environment.set('ADMIN_EMAIL', response.credentials.email);",
									"        pm.environment.set('ADMIN_PASSWORD', response.credentials.password);",
									"        console.log('✅ Admin registrado:', response.credentials.email);",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
